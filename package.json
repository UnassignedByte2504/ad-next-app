{
  "name": "ayla-designs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bun --bun next dev --webpack",
    "build": "bun --bun next build --webpack",
    "start": "bun --bun next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css,yml,yaml}\"",
    "test": "vitest",
    "test:unit": "vitest run --dir tests/unit",
    "test:integration": "vitest run --dir tests/integration",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "bun test:coverage && bun test:e2e",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "cd .. && husky client/.husky",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@floating-ui/react": "^0.27.16",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.5",
    "@mui/material-nextjs": "^7.3.5",
    "@sentry/nextjs": "^10.27.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.26",
    "immer": "^11.0.1",
    "motion": "^12.23.25",
    "next": "16.0.6",
    "next-intl": "^4.1.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.13",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-docs": "^10.1.4",
    "@storybook/addon-links": "^10.1.4",
    "@storybook/nextjs-vite": "^10.1.10",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.14",
    "@vitest/ui": "^4.0.14",
    "eslint": "^9",
    "eslint-config-next": "16.0.6",
    "eslint-plugin-storybook": "10.1.3",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^15.3.0",
    "msw": "^2.12.3",
    "prettier": "^3.4.2",
    "storybook": "^10.1.4",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.14"
  },
  "trustedDependencies": [
    "@sentry/cli",
    "@swc/core",
    "unrs-resolver"
  ]
}
